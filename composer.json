{
    "name": "agungsp/filament-cleave-input",
    "description": "FilamentPHP form component for input formatting using cleave-zen",
    "keywords": [
        "agungsp",
        "laravel",
        "filament-cleave-input"
    ],
    "homepage": "https://github.com/agungsp/filament-cleave-input",
    "support": {
        "issues": "https://github.com/agungsp/filament-cleave-input/issues",
        "source": "https://github.com/agungsp/filament-cleave-input"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Agung Setyo Pribadi",
            "email": "agungsetyopribadi8@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Agungsp\\FilamentCleaveInput\\": "src/",
            "Agungsp\\FilamentCleaveInput\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Agungsp\\FilamentCleaveInput\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agungsp\\FilamentCleaveInput\\FilamentCleaveInputServiceProvider"
            ],
            "aliases": {
                "FilamentCleaveInput": "Agungsp\\FilamentCleaveInput\\Facades\\FilamentCleaveInput"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}